{
    "health_check": false,
    "job_name": "${JOB_NAME}",
    "device_type": "x86",
    "timeout": 18000,
    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "parameters": {
                "overlays": [
                    "${MODULE_ARCHIVE}"
		],
                "kernel": "${KERNEL_ARCHIVE}",
                "nfsrootfs": "${ROOTFS_ARCHIVE}",
		"target_type": "ubuntu"
            }
        },
	{
		"command": "boot_linaro_image"
	},
        {
            "command": "lava_command_run",
            "parameters": {
                "commands": [
                    "ifup eth0",
                    "route -n",
                    "cat /etc/resolv.conf",
                    "echo nameserver 8.8.8.8 > /etc/resolv.conf"
                ]
            }
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_repos": [
                    {
                        "git-repo": "https://github.com/PSRCode/lttng-lava.git",
                        "revision": "WIP",
                        "testdef": "tools/lttng-tools_dep_jenkins_source.yml",
			"parameters": {
				"URCU_ARCHIVE": "${URCU_ARCHIVE}",
				"UST_ARCHIVE": "${UST_ARCHIVE}",
				"BABELTRACE_ARCHIVE": "${BABELTRACE_ARCHIVE}",
				"TOOLS_GIT": "${LTTNGTOOLS_VERSION}"
			}
                    }
                ],
                "timeout": 18000
            }
        }
    ]
}
